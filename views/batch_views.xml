<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="batch_view_form" model="ir.ui.view">
            <field name="name">Batch_batch_form_view</field>
            <field name="model">institution.batch</field>
            <field name="arch" type="xml">
                <form string="Institutional Batch">
                   <sheet>
                       <group>
                           <field name="name" />
                           <field name="course_ids"/>
                           <field name="start_date"/>
                           <field name="end_date"/>
                            <field name="student_id"/>
                    <field name="student_count"/>
                           <field name="status"/>

                       </group>
                   </sheet>
                </form></field>
        </record>

        <record id="batch_view_tree" model="ir.ui.view">
            <field name="name">Batch_batch_tree_view</field>
            <field name="model">institution.batch</field>
            <field name="arch" type="xml">
                <list string="Institutional Batch">


                           <field name="name" />
                           <field name="course_ids"/>
                           <field name="start_date"/>
                           <field name="end_date"/>

                           <field name="status"/>

                </list></field>
        </record>

        <record id="action_batch_view" model="ir.actions.act_window">
            <field name="name">Batch Data</field>
            <field name="res_model">institution.batch</field>
            <field name="view_mode">list,form</field>
        </record>

    </data>

</odoo>