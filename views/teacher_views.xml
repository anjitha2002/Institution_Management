<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
<record id="teacher_view_form" model="ir.ui.view">
    <field name="name">Teacher_teacher_form_view</field>
    <field name="model">institution.teacher</field>
    <field name="arch" type="xml">
        <form string="Institution Teachers">
            <sheet>
                <group>
                    <field name="name"/>
                    <field name="phone"/>
                    <field name="email"/>
                    <field name="experience"/>
                    <field name="course_ids"/>
                </group>
            </sheet>
        </form>
    </field>
</record>

        <record id="teacher_view_tree" model="ir.ui.view">
    <field name="name">Teacher_teacher_tree_view</field>
    <field name="model">institution.teacher</field>
    <field name="arch" type="xml">
        <list string="Institution Teachers">

                    <field name="name"/>

                    <field name="course_ids"/>

        </list>
    </field>
        </record>
        <record id="action_teacher_view" model="ir.actions.act_window">
            <field name="name">Teachers Data</field>
            <field name="res_model">institution.teacher</field>
            <field name="view_mode">list,form</field>
        </record>
    </data>
</odoo>