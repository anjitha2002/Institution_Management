<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="category_view_form" model="ir.ui.view">
            <field name="name">Category_category_form_view</field>
            <field name="model">institution.category</field>
            <field name="arch" type="xml">
                <form string="Institutional categories">
                   <sheet>
                       <group>
                           <field name="name" />
                           <field name="course_ids"/>



                       </group>
                   </sheet>
                </form></field>
        </record>

        <record id="category_view_tree" model="ir.ui.view">
            <field name="name">Category_category_Tree_view</field>
            <field name="model">institution.category</field>
            <field name="arch" type="xml">
                <list string="Institutional Categories">


                           <field name="name" />



                </list></field>
        </record>

        <record id="action_category_view" model="ir.actions.act_window">
            <field name="name">Category Data</field>
            <field name="res_model">institution.category</field>
            <field name="view_mode">list,form</field>
        </record>
    </data>
</odoo>
